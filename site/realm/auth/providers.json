{
    "anon-user": {
        "name": "anon-user",
        "type": "anon-user",
        "disabled": false
    },
    "api-key": {
        "name": "api-key",
        "type": "api-key",
        "disabled": false
    },
    "local-userpass": {
        "name": "local-userpass",
        "type": "local-userpass",
        "config": {
            "autoConfirm": true,
            "confirmationFunctionName": "confirmUser",
            "resetPasswordSubject": "",
            "resetPasswordUrl": "https://incidentdatabase.ai/resetpassword",
            "runConfirmationFunction": false,
            "runResetFunction": false
        },
        "disabled": false
    },
    "oauth2-facebook": {
        "name": "oauth2-facebook",
        "type": "oauth2-facebook",
        "disabled": false,
        "config": {
            "clientId": "%%values.facebookAppId"
        },
        "secret_config": {
            "clientSecret": "%%values.facebookAppSecret"
        },
        "redirect_uris": ["%%values.facebookRedirectUri"]
    }
}
