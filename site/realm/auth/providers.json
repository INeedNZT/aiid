{
    "anon-user": {
        "name": "anon-user",
        "type": "anon-user",
        "disabled": false
    },
    "api-key": {
        "name": "api-key",
        "type": "api-key",
        "disabled": false
    },
    "local-userpass": {
        "name": "local-userpass",
        "type": "local-userpass",
        "config": {
            "autoConfirm": true,
            "confirmationFunctionName": "confirmUser",
            "resetPasswordSubject": "",
            "resetPasswordUrl": "https://incidentdatabase.ai/resetpassword",
            "runConfirmationFunction": false,
            "runResetFunction": false
        },
        "disabled": false
    },
    "oauth2-facebook": {
        "name": "oauth2-facebook",
        "type": "oauth2-facebook",
        "disabled": false,
        "config": {
            "clientId": "%%values.facebookAppId"
        },
        "secret_config": {
            "clientSecret": "facebookAppSecret"
        },
        "metadata_fields": [
            {
                "required": false,
                "name": "name"
            },
            {
                "required": false,
                "name": "first_name"
            },
            {
                "required": false,
                "name": "last_name"
            },
            {
                "required": false,
                "name": "email"
            }
        ],
        "redirect_uris": ["%%values.loginRedirectUri"]
    },
    "oauth2-google": {
        "name": "oauth2-google",
        "type": "oauth2-google",
        "config": {
            "clientId": "%%values.googleClientId"
        },
        "secret_config": {
            "clientSecret": "googleClientSecret"
        },
        "disabled": false,
        "metadata_fields": [
            {
                "required": false,
                "name": "name"
            },
            {
                "required": false,
                "name": "first_name"
            },
            {
                "required": false,
                "name": "last_name"
            },
            {
                "required": false,
                "name": "email"
            }
        ],
        "redirect_uris": ["%%values.loginRedirectUri"]
    }
}
