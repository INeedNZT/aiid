{
    "anon-user": {
        "name": "anon-user",
        "type": "anon-user",
        "disabled": false
    },
    "api-key": {
        "name": "api-key",
        "type": "api-key",
        "disabled": false
    },
    "local-userpass": {
        "name": "local-userpass",
        "type": "local-userpass",
        "config": {
            "autoConfirm": true,
            "confirmationFunctionName": "confirmUser",
            "resetPasswordSubject": "",
            "resetPasswordUrl": "https://incidentdatabase.ai/resetpassword",
            "runConfirmationFunction": false,
            "runResetFunction": false
        },
        "disabled": false
    },
    "oauth2-facebook": {
        "name": "oauth2-facebook",
        "type": "oauth2-facebook",
        "disabled": false,
        "config": {
            "clientId": "%%values.facebookAppId"
        },
        "secret_config": {
            "clientSecret": "facebookAppSecret",
            "clientId": "facebookAppId",
            "redirect_uris": ["loginRedirectUri"]
        },
        "metadata_fields": [
            {
                "required": false,
                "name": "name"
            },
            {
                "required": false,
                "name": "first_name"
            },
            {
                "required": false,
                "name": "last_name"
            },
            {
                "required": false,
                "name": "email"
            }
        ]
    },
    "oauth2-google": {
        "name": "oauth2-google",
        "type": "oauth2-google",
        "secret_config": {
            "clientSecret": "googleClientSecret",
            "clientId": "googleClientId",
            "redirect_uris": "loginRedirectUri"
        },
        "disabled": false,
        "metadata_fields": [
            {
                "required": false,
                "name": "name"
            },
            {
                "required": false,
                "name": "first_name"
            },
            {
                "required": false,
                "name": "last_name"
            },
            {
                "required": false,
                "name": "email"
            }
        ]
    }
}
